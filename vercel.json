{
    "version": 2,
    "builds": [
        {
            "src": "frontend/build/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "backend/dist/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/dist/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
